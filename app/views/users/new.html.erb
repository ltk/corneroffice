<h1>Sign Up</h1>
<%= simple_form_for @user, :url => user_path, :method => :post do |f| %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :highrise_access_token, :as => :hidden %>
  <%= f.button :submit, :value => "Sign Up" %>
<% end %>
